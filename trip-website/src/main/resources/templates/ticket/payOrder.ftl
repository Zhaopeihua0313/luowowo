        var productName =  payInfo.productName;
        if("VST_ORDER" == "FLIGHT_ORDER"){
            var flightName=$(".flight-pay-main .fpm-title").html();
            if(flightName==null){
                productName= $(".pay_info_p").html().split("：")[2].split("<br>")[0];
            }else{
                productName = $(".flight-pay-main .fpm-title").html().split("：")[1];
            }

