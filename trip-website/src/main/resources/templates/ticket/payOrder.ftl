            $('.js_gotopay').removeAttr("disabled");
            return false;
        } else {
            $('.paypass_error_tip').hide();
        }
        $(".payWaiting").show();
        if (payRepeatCheck()) {
            http.sendAjaxMsg("https://pay.lvmama.com/payfront/lvChannelPay/pay.do", urlParser.parseParam(payInfo), paySuccess, fail, true, http.post);
            function paySuccess(data) {
                var resultCode = data.resultCode;
                if (resultCode == "200") { // 支持
                	var datas = data.datas;
                    //支付成功处理
                    //window.location.reload();
                    //alert(datas.url);
                    window.location = datas.url;
                } else if (resultCode == "701") {//支付密码错误
                    if (data.datas.code.name == "FAIL") {
                        //提示错误次数
                        $(".js_pay_password").addClass("input_error");
                        $(".paypass_error_tip").show();
                        $("#sureToPayErrorTip").html("支付密码错误，你还可以输入" + (5 - data.datas.failCount) + "次");
                    } else if (data.datas.code.name == "OVER") {
                        $(".js_pay_password").addClass("input_error");
                        $(".paypass_error_tip").show();
                        $("#sureToPayErrorTip").html('您已输错5次,支付将锁定2小时,您也可使用其他支付方式或者<a class="pay_password_tip pay_password_reset " href="javascript:;" onclick="setPayPass();">重置支付密码</a>');
                        $(".js_gotopay").addClass('btn-forbidden');
                        $(".js_gotopay").attr('disabled',"disabled");
                    } else if (data.datas.code.name == "NOT_EXIST") {
                        $(".js_pay_password").addClass("input_error");
                        $(".paypass_error_tip").show();
                        $("#sureToPayErrorTip").html("支付密码未设置，请先设置支付密码！");
                    }
                } else if (resultCode == "711") {
                    //BLOCK支付阻断
                    $('.paypass_error_tip').show();
                    $('#sureToPayErrorTip').html("账户存在风险，请联系客服");
                    payInfo.cashRiskCode = data.datas.cashRiskCode;
                    payInfo.bonusRiskCode = data.datas.bonusRiskCode;
                    payInfo.lPKRiskCode = data.datas.lPKRiskCode;
                    payInfo.qylRiskCode = data.datas.qylRiskCode;
                    refreshRiskDisplay();
                    return false;
                } else if (resultCode == "712") {
                    //SMS短信验证
                    payInfo.qylRiskCode = data.datas.qylRiskCode;
                    alertSms("712");
                } else if (resultCode == "713") {
                    //SMS短信验证
                    alertSms("713");
