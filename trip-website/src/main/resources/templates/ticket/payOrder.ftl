    urlParser = {
        //将对象转成url 参数
        parseParam: function (param, key) {
            var paramStr = "";
            if (param instanceof String || param instanceof Number || param instanceof Boolean) {
                paramStr += "&" + key + "=" + encodeURIComponent(param);
            } else {
                $.each(param, function (i) {
                    var k = key == null ? i : key + (param instanceof Array ? "[" + i + "]" : "." + i);
                    paramStr += '&' + urlParser.parseParam(this, k);
                });
            }
            return paramStr.substr(1);
        }
    };

    http = {
        get: "get",
        post: "post",
        sendAjaxMsg: function (url, params, successFunction, failFunction, showMask, requestMethod, customCallBackParam) {
            if (typeof requestMethod == 'undefined' || requestMethod == null) {
                requestMethod = 'post';
            }
            $.ajax({
                url: url,
                type: requestMethod,
                data: params,
                /*dataType: "json",*/
                success: function (data, textStatus, jqXHR) {
                    if (successFunction) {
                        successFunction.call(document, data, customCallBackParam);
                    }
                },
                error: function (data) {
                    if (failFunction) {
                        failFunction.call(document, data, customCallBackParam);
                    }
                }
            });
        }
    };

