            productName = $(".pay_info_p").next().find("p:first").html().replace( new RegExp("&nbsp;","g"),"");
        }else if("VST_ORDER" == "INTER_FLIGHT_ORDER"){
            productName = $(".pay_info_p").contents().filter(function() {
                return this.nodeType === 3;
            })[0].nodeValue.trim();
        }
        if(typeof productName != "undefined"){
            payInfo.productName = productName;
        }

        this.style.backgroundColor = '#ffed08';
        if (payInfo.password == "") {
        	$('#sureToPayErrorTip').html("请输入六位支付密码");
            $('.js_gotopay').removeAttr("disabled");
            return false;
        }
        if (payInfo.giftCardList.length == 0 && payInfo.cashAmount == 0 && payInfo.bonusAmount == 0 && payInfo.qylAmount == 0 && payInfo.ybAmount == 0) {
