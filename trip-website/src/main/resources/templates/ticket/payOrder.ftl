                usedAmount = oughtPayAmountFen - giftCardUsedAmount;
            } else {
                usedAmount = giftCard.leftAmt;
            }
            giftCardUsedAmount = giftCardUsedAmount + usedAmount;
            $("#valid_" + giftCard.cardNo).parent().parent().prev().html(parseFloat(usedAmount / 100).toFixed(2));
            giftCard.useAmt = usedAmount;
            if (usedAmount <= 0) {
                $("#valid_" + giftCard.cardNo).removeClass("checked").addClass("checkbox_disabled");
