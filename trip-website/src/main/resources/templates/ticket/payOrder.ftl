        var oughtPayYuan = $('#oughtPayYuan').val();
        var tabLimit = $thisBox.find(".paytablimit").val();
        var bankLimit = $this.find(".paylimit").val();
        if (typeof(oughtPayYuan) != "undefined") {
            oughtPayYuan = oughtPayYuan * 1;
        }
        if (typeof(tabLimit) != "undefined" && tabLimit != "") {
            tabLimit = tabLimit * 1;
        }
        if (typeof(bankLimit) != "undefined" && bankLimit != "") {
            bankLimit = bankLimit * 1;
        }

        if (typeof(tabLimit) != "undefined" && tabLimit != "" && tabLimit < oughtPayYuan) {
            if (paylimitNotice != "") {
                $thisText.show();
            } else {
                $thisText.hide();
            }
        } else if (typeof(bankLimit) != "undefined" && bankLimit != "" && bankLimit < oughtPayYuan) {
            if (paylimitNotice != "") {
                $thisText.show();
            } else {
                $thisText.hide();
            }
        } else {
            $thisText.hide();
        }

