                    var expiredTimeCol;
                    if (item.bakWord === '0') {
                        expiredTimeCol = '<td>' + item.expiredTime + ' <span class="card_tag3">即将过期</span></td>';
                    }else if(item.bakWord === '1') {
                        expiredTimeCol = '<td>' + item.expiredTime + ' <span class="card_tag3">已过期</span></td>';
					}else {
                        expiredTimeCol = '<td>' + item.expiredTime + '</td>';
                    }
                    // checkbox列
                    var ckCol;
                    var orderDetail = pageSize * (currentPage - 1) + i;
                    if (isInvalid) {
                        ckCol = '<td>　　<label class="js_tips" tip-content=""><span class="checkbox checkbox_disabled"><input class="js_checkbox" disabled="" type="checkbox"></span></lable></td>';
                    } else {
                        ckCol = '<td>　　<label class="js_tips" tip-content=""><span class="checkbox" id="valid_' + item.cardNo + '" orderDetail="' + orderDetail + '"><input class="js_checkbox" type="checkbox"></span></lable></td>';
                    }
