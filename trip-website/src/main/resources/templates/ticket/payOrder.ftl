        var param = {
            orderId: "19107323211",
            bizType: "VST_ORDER",
            status: state,
            categoryId:"11",
            productId:"181554",
            pageSize: pageSize,
            currentPage: currentPage
        };

        http.sendAjaxMsg("https://pay.lvmama.com/payfront/lvChannelPay/getUserLvmamaCardList.do", urlParser.parseParam(param), success, fail, false, http.post);

        function success(data) {
            if (data.resultCode === '200') {
                var page = data.datas;
                var currentPage = page.currentPage;
                var totalResultSize = page.totalResultSize;
                var totalPageNum = page.totalPageNum;
                var items = page.items;

                // checkbox head
                var ckColHd = '';
                $(".js_clear_check").hide();
                if (totalResultSize == 0) {  //todo 当礼品卡数量为0时，不显示相应礼品卡类型
                    if (isInvalid) {
                        $("#unavailable_card_tb").hide().prev().hide();
                    } else {
                        $("#available_card_tb").hide().prev().hide();
                    }
                    return;
                }

                if (isInvalid) {
                    $('#invalid_count').text(totalResultSize);
                    if (totalResultSize > pageSize) {
                        $("#unavailable_page").show();
                    }
                } else {
                	
                    $('#valid_count').text(totalResultSize);
                    if (totalResultSize > pageSize) {
                        ckColHd = '<label>全选<span class="checkbox "><input class="js_checkbox js_allchecked" type="checkbox"></span></label>';
                        $("#available_page").show();
                        $(".js_clear_check").show();
                        $(document).off('click', '.js_allchecked');
                        $(document).on('click', '.js_allchecked', allCheckedClickHandler);
                    }
                }

                //todo 新增卡种名称列
                var headRow = '<tr>' +
                    '<th width="50"></th>' +
                    '<th>剩余金额(元）</th>' +
                    '<th width="190">有效期</th>' +
                    '<th>礼品卡号</th>' +
                    '<th>卡种</th>' +
                    '<th>当前订单使用(元）</th>' +
