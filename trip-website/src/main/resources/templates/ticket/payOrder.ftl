                    //重复支付
                    window.location.reload();
                }else{
                     $('.paypass_error_tip').show();
                	 $('#sureToPayErrorTip').html(data.resultMsg);
                    setTimeout(function(){
                        window.location.reload();
                    },2000);//延时2s进行重新加载

                }
                $(".payWaiting").hide();
                $('.js_gotopay').removeAttr("disabled");
            }
        }
    }

    //礼品卡绑定
    function bindCardClickHandler(e) {
        var $addCardErrorTipDiv = $(".addCard_error_tip")
        var $bindCardErrorTip = $("#bindCardErrorTip");
        var $bindCardYzm = $("#bindCardYzm");
        var needYzm = $bindCardYzm.is(":visible");
        var yzm = $("#yzm").val();
        var cardNo = $("#cardNo").val();
        var passwd = $("#passwd").val();
        var orderId = "19107323211";
        var bizType = "VST_ORDER";
        var categoryId = "11";
        var productId = "";

        if (needYzm) {
            if (yzm.trim().length === 0) {
                $bindCardErrorTip.text("无效的验证码！");
                $addCardErrorTipDiv.show();
                return;
            }
        }

        //校验当前礼品卡是否支持
        var param = {cardNo: cardNo, password: passwd, checkCode: yzm, orderId: orderId, bizType: bizType,categoryId:categoryId,productId:productId};
        http.sendAjaxMsg("https://pay.lvmama.com/payfront/lvChannelPay/validateGiftCard.do", urlParser.parseParam(param), validateSuccess, fail, true, http.post);

        //校验礼品卡是否支持的成功回调
        function validateSuccess(data) {
            var resultCode = data.resultCode;
            if (resultCode == '200') { // 支持
                clearErrorTip();
                http.sendAjaxMsg("https://pay.lvmama.com/payfront/lvChannelPay/bindGiftCard.do", urlParser.parseParam(param), bindSuccess, fail, true, http.post, "available_card_tb");
            } else if (resultCode == '508') { // 需要输入验证码
                $bindCardYzm.show();
            } else if (resultCode == '510') { // 验证码错误
                yzmErrorHandler(data);
            } else if (resultCode == '511') { // 不适用于当前订单
                cardNotSuitable4Order();
            } else { // 其他错误
                showErrorTip(data);
            }
            refreshYzm();

            //礼品卡不适用当前订单的处理逻辑
            function cardNotSuitable4Order() {
                pandora.dialog({
                    title: "绑定礼品卡",
                    content: $('#lpk_bind'),
                    width: 400,
                    wrapClass: "dialog-lpk-bind", //模板名称
                    ok: function () {
                        http.sendAjaxMsg("https://pay.lvmama.com/payfront/lvChannelPay/bindGiftCard.do", urlParser.parseParam(param), bindSuccess, fail, true, http.post, "unavailable_card_tb");
                    },
                    okClassName: 'btn btn-orange',
                    okValue: '确认绑定',
                    cancel: function () {
                    },
                    cancelClassName: 'btn',
                    cancelValue: '取消'
                });
            }
        }

        //礼品卡绑定的成功回调
        function bindSuccess(data, refreshTbId) {
            var resultCode = data.resultCode;
            $("#passwd,#bindCardYzm").val("");
            if (resultCode == '200') { // 绑定成功
                clearErrorTip();
                $("#" + refreshTbId).show().prev().show();
                $("#cardNo").val("");
                loadLvmamaCard(refreshTbId, defaultPageSize, 1);
            } else if (resultCode == '508') { // 需要输入验证码
                $bindCardYzm.show();
            } else if (resultCode == '510') { // 验证码错误
                yzmErrorHandler(data);
            } else { // 其他错误
                showErrorTip(data);
            }
            refreshYzm();
        }

        //返回验证码错误的处理
        function yzmErrorHandler(data) {
            if ($bindCardYzm.is(":visible")) {
                showErrorTip(data);
            } else {
                $bindCardYzm.show();
            }
        }

        //清空错误提示
