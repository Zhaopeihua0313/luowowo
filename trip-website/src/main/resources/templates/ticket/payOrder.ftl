                    //todo 新增卡种名称列
                    var rowData = $('<tr></tr>').append(amountTypeCol)
                        .append('<td>' + item.leftAmtYuan + '</td>')
                        .append(expiredTimeCol)
                        .append('<td>' + item.cardNo + '</td>')
                        .append('<td> <label class="js_tips" tip-content="'+item.bindCategoryDescription+'">' + item.cardTypeName + '</lable></td>')
